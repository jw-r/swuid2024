generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Designer {
  id          Int     @id @default(autoincrement())
  order       Int
  avatar      String
  name        String
  classNumber Int
  fields      String
  englishName String
  email       String  @unique
  instagram   String?
  behance     String?
  thought     String
}
